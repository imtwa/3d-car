{
  "name": "vue-car-front",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint:prettier": "prettier --check .",
    "lintfix": "prettier --write --list-different .",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:prettier"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,vue}": [
      "npm run lintfix"
    ],
    "src/**/*.{scss,css}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@types/three": "^0.174.0",
    "axios": "^1.8.1",
    "pinia": "^3.0.1",
    "three": "^0.174.0",
    "vue": "^3.5.13",
    "vue-router": "4"
  },
  "devDependencies": {
    "@types/node": "^22.13.8",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-vue": "^9.32.0",
    "postcss": "^8.5.3",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.5.3",
    "sass": "^1.85.1",
    "tailwindcss": "^4.0.9",
    "typescript": "~5.7.2",
    "vite": "^6.2.0",
    "vue-tsc": "^2.2.4"
  }
}
