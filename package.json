{
  "name": "vue-car-front",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint:prettier": "prettier --check .",
    "lintfix": "prettier --write --list-different .",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:prettier"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "npm run lintfix"
    ],
    "src/**/*.{scss,css}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "axios": "^1.8.1",
    "element-plus": "^2.4.4",
    "mockjs": "^1.1.0",
    "pinia": "^3.0.1",
    "three": "^0.174.0",
    "vue": "^3.5.13",
    "vue-router": "4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-vue": "^9.32.0",
    "husky": "^8.0.0",
    "install": "^0.13.0",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.35",
    "prettier": "^3.5.3",
    "sass": "^1.85.1",
    "tailwindcss": "^3.4.1",
    "vite": "^6.2.0"
  }
}
